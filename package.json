{
  "name": "flashx",
  "version": "1.0.6",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/firdausyusofs/project-torrent"
  },
  "dependencies": {
    "@electron/remote": "^1.0.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "electron-acrylic-window": "^0.5.8",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^4.3.9",
    "lodash.debounce": "^4.0.8",
    "popcorn-api": "^1.1.1",
    "pouchdb-node": "^7.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sass": "^1.42.1",
    "styled-components": "^5.2.1",
    "video.js": "^7.11.8",
    "vlc-command": "^1.2.0",
    "web-vitals": "^1.0.1",
    "webtorrent": "^0.115.2"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "electron:build": "yarn build && electron-builder -w --x64 -c.extraMetadata.main=build/main.js",
    "electron:publish": "yarn build && electron-builder -w --x64 -p always -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "build": {
    "extends": null,
    "appId": "com.fy.flashx",
    "productName": "FlashX",
    "publish": [
      "github"
    ],
    "files": [
      "dist/**/*",
      "build/**/*",
      "public/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "icon": "build/icon.png"
    },
    "win": {
      "icon": "build/icon.png"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "perMachine": true
    },
    "directories": {
      "buildResources": "assets"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^11.2.1",
    "electron-builder": "^22.10.5",
    "wait-on": "^5.2.1"
  }
}
